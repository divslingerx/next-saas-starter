/**
 * Main library function
 * @param input - Input data
 * @returns Processed result
 */
export function process<T>(input: T): import('./types').Response<T> {
  try {
    // Process the input
    return {
      success: true,
      data: input,
    };
  } catch (error) {
    return {
      success: false,
      error: error instanceof Error ? error : new Error('Unknown error'),
    };
  }
}